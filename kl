    local function CreateDropdownSetting(optionName, saveKey, defaultIndex, values, tooltip)
        Settings:AddDropdown(optionName, {
            Values = values,
            Default = defaultIndex,
            Text = optionName,
            Tooltip = tooltip,
        })
        
        Options[optionName]:OnChanged(function(value)
            _G.Save[saveKey] = value
            saveSettings()
        end)
    end

