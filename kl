	local function GetSubPrefix(G)
		G = tostring(G):gsub(" ", "")
		local E = ""
		if #G == 2 then
			local Z = string.sub(G, #G, #G + 1)
			E = Z == "1" and "st" or Z == "2" and "nd" or Z == "3" and "rd" or "th"
		end
		return E
	end

	local q = "%A, %B"
	local m = os.date(" %d", os.time())
	local s = ", %Y."
	q = os.date(q, os.time()) .. m .. GetSubPrefix() .. os.date(s, os.time())

